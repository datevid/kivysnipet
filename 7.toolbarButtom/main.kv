<Screen1>:
    MDBoxLayout:
        orientation:"vertical"
       
        MDTopAppBar:
            title: "Todo List - Menu"
            md_bg_color:app.theme_cls.primary_light

        MDLabel:
            text: "contenido del label"
            halign:"center"
        
        MDBottomAppBar:
            MDTopAppBar:
                title:"bottom"
                icon:"plus"
                type:"bottom"
                left_action_items:[["menu",lambda x:app.fnBottomMenu()]]
                on_action_button: app.fnButtomPlus()
                mode:"end"
