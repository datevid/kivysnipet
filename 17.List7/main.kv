<InterfazWithScreen1>:
    MDScreen:
        MDTopAppBar:
            pos_hint: {"top": 1}
            elevation: 4
            title: "MDNavigationDrawer"
            left_action_items: [["menu", lambda x: navDrawerId.set_state("open")]]
        MDNavigationLayout:
            MDScreenManager:
                id: screenManagerId
                Screen1:
                Screen2:
                Screen3:
        MDNavigationDrawer:
            id: navDrawerId
            radius: (0, 0, 0, 0)
            MDNavigationDrawerMenu:

                MDNavigationDrawerHeader:
                    title: "Header title"
                    title_color: "#4a4939"
                    text: "Header text"
                    spacing: "4dp"
                    padding: "12dp", 0, 0, "56dp"

                MDNavigationDrawerLabel:
                    text: "Mail"

                MDNavigationDrawerItem:
                    icon: "gmail"
                    right_text: "+99"
                    text_right_color: "#4a4939"
                    text: "Screen1"
                    on_press:
                        #root.navDrawerId.set_state("close")
                        navDrawerId.set_state("close")
                        root.changeScreenInner("scr1")
                        #root.screen_manager.current = "scr1"

                MDNavigationDrawerItem:
                    icon: "send"
                    text: "Screen2"
                    on_press:
                        navDrawerId.set_state("close")
                        #root.screen_manager.current = "scr2"
                        root.changeScreenInner("scr2")

                MDNavigationDrawerItem:
                    icon: "send"
                    text: "Screen3"
                    on_press:
                        navDrawerId.set_state("close")
                        #root.screen_manager.current = "scr3"
                        root.changeScreenInner("scr3")

                MDNavigationDrawerDivider:

                MDNavigationDrawerLabel:
                    text: "Labels"

                MDNavigationDrawerItem:
                    icon: "information-outline"
                    text: "Label"

                MDNavigationDrawerItem:
                    icon: "information-outline"
                    text: "Label"

<Screen1>:
#MDScreen:
    name:"scr1"
    BoxLayout:
        orientation:"vertical"
        spacing:"25dp"
        padding:[12,80,12,12]

        MDScrollView:
            MDList:
                OneLineListItem:
                    text: "List n 1"
                    on_press:
                        root.listclick()
                OneLineListItem:
                    text: "List n 2"
                    on_press:
                        root.listclick()


<Screen2>:
#MDScreen:
    name: "scr2"
    MDLabel:
        text: "Sscr2"
        #halign: "center"

<Screen3>:
#MDScreen:
    name: "scr3"
    MDLabel:
        text: "scr3"
        #halign: "center"

<ListItemWithCheckbox>:
    IconLeftWidget:
        icon: root.icon
    RightCheckbox:


